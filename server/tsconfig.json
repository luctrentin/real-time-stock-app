{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES2020",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ],
  "include": [
    "./*.ts",
    "./src/**/*.ts"
, "server.js"  ]
}